{
  "name": "infoexam/eloquent",
  "description": "Infoexam eloquent.",
  "type": "library",
  "homepage": "https://github.com/Infoexam/eloquent",
  "license": "MIT",
  "authors": [
    {
      "name": "BePsvPT",
      "email": "freedom@csie.io"
    }
  ],
  "require": {
    "php": "^7.0",
    "illuminate/auth": "^5.3",
    "illuminate/database": "^5.3",
    "illuminate/notifications": "^5.3",
    "illuminate/support": "^5.3",
    "infoexam/media": "dev-master",
    "infoexam/password": "dev-master",
    "nesbot/carbon": "^1.21"
  },
  "require-dev": {
    "codeclimate/php-test-reporter": "^0.3",
    "orchestra/testbench": "^3.3",
    "phpunit/phpunit": "^5.7"
  },
  "repositories": [
    {"type": "vcs", "url": "https://github.com/Infoexam/password.git"},
    {"type": "vcs", "url": "https://github.com/Infoexam/media.git"}
  ],
  "autoload": {
    "psr-4": {
      "Infoexam\\Eloquent\\": "src/"
    }
  },
  "autoload-dev": {
    "classmap": [
      "tests"
    ]
  },
  "config": {
    "optimize-autoloader": true,
    "sort-packages": true,
    "notify-on-install": false
  }
}
